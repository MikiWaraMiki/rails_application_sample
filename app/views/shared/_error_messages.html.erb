<% if object.errors.any? %>
    <div id="error_explanation">
        <div class="alert alert-danger">
            <% #pluralize is helper. if first arg value is plural, translation automacaly%>
            The form contains <%= pluralize(objcet.errors.count, "error") %>
        </div>
        <ul>
            <% objcet.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
        </ul>
    </div>
<% end %>
